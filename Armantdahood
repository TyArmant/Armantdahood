local Tab = Window:NewTab("Trash Talk")
local Section = Tab:NewSection("Trash Talk (J)")
Section:NewButton("Enable", "ButtonInfo", function()
    print("Clicked")
    
    local key = "j" -- the toxic key
_G.Toxic = false
 
local words = 
	{
		"ez kid",
		"ezzz",
		"l",
		"loser",
		"ez win",
		"owned",
		"suck",
		"clown",
		"ez clown kid",
		"ur bad",
		"seed",
		"kid",
		"ngl ur trash",
        "trash",
	}
 
local player = game:GetService("Players").LocalPlayer
local mouse = player:GetMouse()
 
local event = game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest")
 
mouse.KeyDown:Connect(function(keybind)
	if _G.Toxic == false then
		if keybind == key then
			event:FireServer(words[math.random(#words)], "All")
		end
	end
end)
end)









